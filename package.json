{
  "name": "@jemoo/websdk",
  "version": "0.0.1",
  "description": "websdk",
  "author": "movinliao@163.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/movins/websdk.git"
  },
  "main": "dist/websdk.min.js",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "dev": "webpack --config build/webpack.dev.config.js && http-server ./ -p 8090",
    "dist": "webpack --config build/webpack.dist.config.js"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "dependencies": {
    "eventemitter3": "^2.0.0",
    "raw-loader": "^0.5.1",
    "yarn": "^1.22.10"
  },
  "devDependencies": {
    "@movinliao/eslint-config-mlc": "^0.0.4",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "http-server": "^0.10.0",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "compression-webpack-plugin": "^1.1.12",
    "webpack": "^3.6.0"
  },
  "browser": {
    "ws": false,
    "fs": false
  },
  "eslintConfig": {
    "extends": "@movinliao/eslint-config-mlc",
    "globals": {
      "__VERSION__": true,
      "__DEV__": true
    },
    "rules": {
      "camelcase": 0
    }
  },
  "eslintIgnore": [
    "/dev",
    "/output",
    "/node_modules",
    "/src/data",
    "/src/proto/pb",
    "/build/proto"
  ]
}
